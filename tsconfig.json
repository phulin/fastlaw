{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"noEmit": true,
		"types": ["@cloudflare/workers-types"],
		"isolatedModules": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules", "dist", ".output"]
}

{
	"name": "fastlaw",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"start": "vite preview",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"seed:pinecone": "node scripts/seed_pinecone.mjs",
		"r2:ingest:local": "tsx scripts/ingest_r2_local.ts",
		"migrate:local": "wrangler d1 migrations apply fastlaw --local",
		"migrate:remote": "wrangler d1 migrations apply fastlaw --remote"
	},
	"dependencies": {
		"@google/genai": "^1.38.0",
		"hono": "^4.6.4",
		"marked": "^12.0.2",
		"solid-js": "^1.9.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@cloudflare/vite-plugin": "^1.22.1",
		"@cloudflare/workers-types": "^4.20250129.0",
		"@types/better-sqlite3": "^7.6.12",
		"@types/node": "^22.13.5",
		"better-sqlite3": "^11.2.0",
		"tsx": "^4.21.0",
		"typescript": "^5.7.3",
		"vite": "^6.1.1",
		"vite-plugin-solid": "^2.10.0",
		"wrangler": "^4.61.1"
	},
	"packageManager": "yarn@4.12.0",
	"engines": {
		"node": ">=22"
	}
}

{
	"name": "@fastlaw/ingest",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "NODE_EXTRA_CA_CERTS=certs/godaddy-g2.pem wrangler dev --persist-to ../web/.wrangler/state",
		"build": "wrangler deploy --dry-run --outdir=dist",
		"deploy": "wrangler deploy",
		"typecheck": "../../node_modules/.bin/tsc --noEmit && cd container-rust && cargo check",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@cloudflare/containers": "^0.1.0",
		"@zip.js/zip.js": "^2.8.16",
		"hono": "^4.6.4",
		"htmlparser2": "^9.1.0",
		"sax-wasm": "^3.1.3",
		"xxhash-wasm": "^1.1.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250129.0",
		"vitest": "^3.0.0",
		"wrangler": "^4.63.0"
	}
}
